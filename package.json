{
  "version": "3.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc && bob build --single",
    "test": "jest",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "lint-staged": "lint-staged",
    "eslint": "eslint",
    "ci:eslint": "eslint src/**/*",
    "release": "changeset publish"
  },
  "peerDependencies": {},
  "name": "@n1ru4l/push-pull-async-iterable-iterator",
  "author": {
    "name": "Laurin Quast",
    "email": "laurinquast@googlemail.com",
    "url": "https://github.com/n1ru4l"
  },
  "repository": "https://github.com/n1ru4l/push-pull-async-iterable-iterator",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "1 KB"
    }
  ],
  "lint-staged": {
    "*.{js}": [
      "eslint",
      "git add -f"
    ],
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add -f"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "2.17.0",
    "@size-limit/preset-small-lib": "5.0.3",
    "@types/jest": "27.0.1",
    "@types/zen-observable": "0.8.3",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "bob-the-bundler": "1.5.1",
    "changesets": "1.0.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.2",
    "jest": "27.0.6",
    "lint-staged": "11.2.6",
    "patch-package": "6.4.7",
    "size-limit": "5.0.3",
    "ts-jest": "27.0.5",
    "typescript": "4.3.5",
    "zen-observable": "0.8.15"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
